    "import matplotlib.pyplot as plt\n",
    "\n",
    "# set a grey background\n",
    "sns.set(style='darkgrid')\n",
    "df = sns.load_dataset('iris')\n",
    "\n",
    "fig = sns.kdeplot(df['petal_length'], shade=True, color=\"y\")\n",
    "fig = sns.kdeplot(df['sepal_length'], shade=True, color=\"g\")\n",
    "\n",
    "plt.show()"
